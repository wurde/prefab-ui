{
  "name": "@wurde/pf-bar-chart",
  "version": "0.1.0",
  "description": "A simple bar chart.",
  "private": false,
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "rimraf build/* dist/*",
    "build": "tsc --project tsconfig.json && npm run webpack",
    "webpack": "webpack-cli --config webpack.config.js --mode production --entry ./build/index.js --output ./dist/index.js",
    "start:dev": "http-server ./src || nodemon -e ts -x npm run build",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/wurde/prefab-ui.git",
    "directory": "packages/pf-bar-chart"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  },
  "bugs": {
    "url": "https://github.com/wurde/prefab-ui/issues"
  },
  "keywords": [
    "d3",
    "bar-chart"
  ],
  "author": "Andy Bettisworth & Moses Samuel",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@storybook/addon-actions": "^6.0.20",
    "@storybook/addon-essentials": "^6.0.20",
    "@storybook/addon-links": "^6.0.20",
    "@storybook/web-components": "^6.0.20",
    "@types/d3": "^5.7.2",
    "babel-loader": "^8.1.0",
    "http-server": "^0.12.3",
    "nodemon": "^2.0.4",
    "react-is": "^16.13.1",
    "rimraf": "^3.0.2",
    "ts-loader": "^8.0.3",
    "typescript": "^4.0.2",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "d3": "^6.1.0",
    "lit-html": "^1.3.0"
  }
}
