{
  "name": "pf-icons",
  "version": "0.1.1",
  "description": "A suite of icons.",
  "private": false,
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "rimraf build/* dist/*",
    "build": "tsc --project ./tsconfig.json",
    "build-backup": "tsc --project ./tsconfig.json && npm run webpack",
