    "directory": "packages/pf-icons"
  },
  "keywords": [
    "web-component",
    "lit-element",
    "icon"
  ],
  "author": "Andy Bettisworth",
  "license": "MIT",
  "dependencies": {
    "lit-element": "^2.4.0"
  },
  "files": [
    "dist/**"
  ]
}
