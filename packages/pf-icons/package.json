{
  "name": "@wurde/pf-icons",
  "version": "0.2.0",
  "description": "A suite of icons.",
  "private": false,
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "rimraf build/* dist/*",
    "build": "tsc --project tsconfig.json && npm run webpack",
